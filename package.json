    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.6",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.11.2",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^4.0.0",
    "es6-promise": "^4.0.5",
    "formsy-react": "^0.18.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean-css": "^2.0.5",
    "gulp-gh-pages": "^0.5.4",
    "gulp-notify": "^2.2.0",
